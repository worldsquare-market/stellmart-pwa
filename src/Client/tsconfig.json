{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitOnError": true,
    "importHelpers": true,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "alwaysStrict": true,
    "jsx": "react",
    "jsxFactory": "h",
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ]
}
